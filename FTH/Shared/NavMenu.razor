<Bar 
    Breakpoint="Breakpoint.Desktop"
    Background="Background.Dark"
    ThemeContrast="ThemeContrast.Dark"

     >
    <BarBrand>
        <BarItem>
        <BarLink To="/">
            #FightForTheHomeless
        </BarLink>
        </BarItem>
    </BarBrand>
    <BarToggler  />
    <BarMenu>
        <BarStart>
            <BarItem>
                <BarLink To="/">
                    Home
                </BarLink>
            </BarItem>
            <BarItem>
                <BarLink To="/EventBrite">
                    Donate
                </BarLink>
            </BarItem>
            <BarItem>
                <BarDropdown>
                    <BarDropdownToggle> The Charities</BarDropdownToggle>
                    <BarDropdownMenu>
                        <BarDropdownItem To="https://www.stgeorgescrypt.org.uk/" Target="Target.Blank">St. Georges Crypt</BarDropdownItem>
                        <BarDropdownItem To="https://www.pathdevon.org/" Target="Target.Blank">Path Devon</BarDropdownItem>
                    </BarDropdownMenu>
                </BarDropdown>
            </BarItem>
            <BarItem>
                <BarLink To="/Gallery">
                    Gallery
                </BarLink>
            </BarItem>
            <BarItem>
                <BarLink To="/Rules">
                    The Rules
                </BarLink>
            </BarItem>
            <BarItem>
                <BarItem>
                    <BarDropdown>
                        <BarDropdownToggle>Videos</BarDropdownToggle>
                        <BarDropdownMenu>
                            <BarDropdownItem To="/Trailer">Trailer</BarDropdownItem>
                            <BarDropdownItem To="Appeal">An Urgent Appeal</BarDropdownItem>
                        </BarDropdownMenu>
                    </BarDropdown>
                </BarItem>

                
            </BarItem>

        </BarStart>
    </BarMenu>

</Bar>
